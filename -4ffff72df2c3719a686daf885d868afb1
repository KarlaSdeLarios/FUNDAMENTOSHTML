<script type="text/javascript">
    $(document).ready(function () {
        AccountFacade.setSid('null');
        setIntervalForUserChecking();
    });
    var currentUserId;
    function checkUser(){
        var newValueOfUserId = $.cookie("Login");
        if (newValueOfUserId != currentUserId){
            if (currentUserId != "\"\""){
                alert('Se ha cambiado el usuario. As\u00ed que le reenviamos a la p\u00e1gina principal.  ');
                document.location.href = "http://www.4shared.com/";
            }
        }
    }

    function setIntervalForUserChecking(){
        currentUserId = $.cookie("Login");
        //setInterval('checkUser()', 10000);
    }
</script>
















