      var windowname="Feedback";
      var url="/feedback.jsp?aLink=http://e.4shared.com/linkerror.jsp";
      OpenWindow = window.open(url,windowname,'toolbar=no,scrollbars=yes,resizable=yes,width=500,height=460,left=50,top=50');
      OpenWindow.focus();
    }

