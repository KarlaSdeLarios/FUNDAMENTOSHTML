<script type="text/javascript">
    function sendVerificationLetter(){
        $.get('http://www.4shared.com/account/sendEmailVerification.jsp?callback=?&globSysLang=es', {ajax: 'true'} ,function(data){
            var msg;
            if (data.ok) {
                msg = data.verificationMailSentMsg;
            }else{
                msg = data.errorMessage;
            }
            AccountFacade.alert({title:"Check your e-mail", content:msg});
        }, "jsonp");
    }


    function sendVerificationLetterAndGoToVerificationPage(){
        $.getJSON('/account/sendEmailVerification.jsp?globSysLang=es', {ajax: 'true'} ,function(data){
            var msg;
            if(data.ok && data.ok === true){
                msg = data.verificationMailSentMsg;
            }else{
                msg = data.errorMessage;
            }
            alert(msg);
            window.location.href = 'http://www.4shared.com/web/acc/verifyemail' ;
        });
    }

</script>



  






